{
	"name": "documentation-site",
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"build": "vocs build",
		"check-links": "bun ./check-links.ts",
		"check-markdown": "remark . --quiet",
		"ci": "run-p check-markdown check-links && bun run build",
		"dev": "vocs dev",
		"format-markdown": "remark . --output --use sort-definitions --use remark-lint-match-punctuation --use remark-lint-no-url-trailing-slash",
		"preview": "vocs preview"
	},
	"dependencies": {
		"@biomejs/biome": "1.9.4",
		"@risc0/ui": "0.0.206",
		"@types/react": "18.3.12",
		"markdown-link-check": "3.13.6",
		"npm-run-all": "4.1.5",
		"react": "18.3.1",
		"react-dom": "18.3.1",
		"remark": "15.0.1",
		"remark-cli": "12.0.1",
		"remark-inline-links": "7.0.0",
		"remark-lint-heading-capitalization": "1.2.0",
		"remark-lint-heading-case": "1.0.0",
		"remark-lint-match-punctuation": "0.2.1",
		"remark-lint-no-url-trailing-slash": "3.0.1",
		"remark-math": "6.0.0",
		"remark-preset-lint-consistent": "6.0.0",
		"remark-preset-lint-markdown-style-guide": "6.0.0",
		"remark-preset-lint-recommended": "7.0.0",
		"remark-remove-url-trailing-slash": "2.0.0",
		"remark-sort-definitions": "2.0.0",
		"remark-toc": "9.0.0",
		"remark-wiki-link": "2.0.1",
		"typescript": "5.6.3",
		"vocs": "1.0.0-alpha.62"
	},
	"packageManager": "bun@1.1.34"
}
