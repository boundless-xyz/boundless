      "targets": [
        {
          "datasource": "postgres-taskdb",
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT\n  t.job_id,\n  MIN(t.started_at) AS start_time,\n  i.total_cycles,\n  EXTRACT(EPOCH FROM (MAX(t.updated_at) - MIN(t.started_at))) AS elapsed_sec,\n  CASE WHEN EXTRACT(EPOCH FROM (MAX(t.updated_at) - MIN(t.started_at))) > 0\n       AND i.total_cycles IS NOT NULL\n       THEN CAST(i.total_cycles AS decimal) / EXTRACT(EPOCH FROM (MAX(t.updated_at) - MIN(t.started_at)))\n  END AS hz\nFROM tasks t\nLEFT JOIN (\n  SELECT job_id, output->'total_cycles' AS total_cycles\n  FROM tasks WHERE task_id = 'init'\n) i ON i.job_id = t.job_id\nGROUP BY t.job_id, i.total_cycles\nORDER BY start_time DESC\n",
          "refId": "A",
          "sql": {
            "columns": [
              {
                "parameters": [],
                "type": "function"
              }
            ],
